<h3>Edit Invoice</h3><br>
<div class="container">
        <div class="form-group">
<%= form_for (@invoice), url: invoice_path, method: :patch do |f| %>
<%- render partial: 'sessions/errors', locals: {obj: @invoice} %>
<%= render partial: 'form', locals: {f: f} %>

<label>Select Musician </label>
<%= f.collection_select :user_id, User.all, :id, :name, include_blank: 'Please Choose the Musician' %><br>

<%= f.submit %>
<% end %>

    </div>
</div>