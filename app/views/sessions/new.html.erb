<h1>Login</h1><br>
<%= form_with(url: "/login", method: "post") do %>
<p>
<%= label_tag :username%>
<%= text_field_tag :username %>
</p>

<p>
<%= label_tag :password %>
<%= password_field_tag :password %>
</p>

<%= submit_tag "Login"%>
<% end %><br>

<%= button_to "Log in with Google", '/auth/google_oauth2'  %><br>
<%= button_to "Log in with Facebook", '/auth/facebook' %>

